@using System.Diagnostics
@model KidsClubApplication.Models.Booked
@{
    ViewData["Title"] = "EventBook";
    List<Event> events = ViewBag.EventList;
    List<SelectListItem> items = new List<SelectListItem>();
    foreach (var e in events)
    {
        items.Add(new SelectListItem(e.EventTitle, e.EventTitle));
    }


    foreach (var item in items) Debug.WriteLine(item.Text);

}



<!-- Contact Form Section -->
<section class="contact-form-section">
    <div class="auto-container">
        <div class="contact-form">
            <div class="sec-title text-center">
                <span class="title">Would you like to join a event?</span>
                <h2>Book a event!</h2>
            </div>

            <section class="about-section-two" id="about-section">
                <div class="auto-container">
                    <div class="parallax-scene parallax-scene-1 anim-icons">
                        <span data-depth="0.20" class="parallax-layer icon icon-rainbow-3"></span>
                        <span data-depth="0.40" class="parallax-layer icon icon-star-2"></span>
                        <span data-depth="0.30" class="parallax-layer icon icon-star-5"></span>
                        <span data-depth="0.50" class="parallax-layer icon icon-plane-2"></span>
                    </div>
                </div>

            </section>

            <form asp-controller="EventBook" asp-action="EventMessage" method="post" id="contact-form">
                <div class="row">
                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        <input type="text" class="form-control" id="EFullName" name="EFullName" placeholder="Your Name">
                        <label for="EFullName">Your Name</label>
                    </div>

                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        <input type="text" class="form-control" id="PNumber" name="PNumber" placeholder="Your number">
                        <label for="PNumber">Your Number</label>
                    </div>

                    <div class="form-group col-lg-6 col-md-12 col-sm-12">
                        <select class="form-control" asp-items="@items" id="ESubject" name="ESubject"></select>
                        <label for="ESubject">Joining Event</label>
                    </div>

                    <div class="form-group col-lg-12 col-md-12 col-sm-12">
                        <textarea class="form-control" placeholder="Leave a comment here" id="EMessage" name="EMessage"></textarea>
                        <label for="EMessage">Leave a message</label>
                    </div>
                </div>

                <div class="form-group col-lg-12 col-md-12 col-sm-12 text-center">
                    <button class="theme-btn btn-style-one" type="submit" name="submit-form">Join Event</button>
                </div>

            </form>
        </div>


    </div>
</section>